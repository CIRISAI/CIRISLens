        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT \n  labels->>'service' AS \"Service\",\n  SUM(CASE WHEN metric_name = 'service.requests' THEN value ELSE 0 END)::INT AS \"Total Requests\",\n  SUM(CASE WHEN metric_name = 'service.errors' THEN value ELSE 0 END)::INT AS \"Total Errors\",\n  AVG(CASE WHEN metric_name = 'service.error_rate' THEN value ELSE NULL END)::NUMERIC(5,4) AS \"Error Rate\",\n  AVG(CASE WHEN metric_name = 'service.uptime' THEN value ELSE NULL END)::INT AS \"Uptime (s)\",\n  MAX(CASE WHEN metric_name = 'service.healthy' THEN value ELSE 0 END)::INT AS \"Healthy\"\nFROM agent_metrics\nWHERE \n  agent_name = '$agent'\n  AND metric_name LIKE 'service.%'\n  AND timestamp >= NOW() - INTERVAL '5 minutes'\n  AND labels->>'service' IS NOT NULL\nGROUP BY labels->>'service'\nORDER BY \"Total Requests\" DESC",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "agent_metrics",
          "timeColumn": "timestamp",
          "timeColumnType": "timestamp",
          "where": []
        }
      ],
      "title": "Service Health Overview",
      "type": "table"
