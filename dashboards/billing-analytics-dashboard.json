          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  timestamp as \"Time\",\n  level as \"Level\",\n  CASE\n    WHEN message LIKE '%/v1/billing/credits/check%' THEN 'Credit Check'\n    WHEN message LIKE '%/v1/billing/litellm/usage%' THEN 'LLM Usage'\n    WHEN message LIKE '%/v1/billing/charges%' THEN 'Charge'\n    WHEN message LIKE '%/v1/integrity%' THEN 'Integrity'\n    WHEN message LIKE '%oauth%' THEN 'OAuth'\n    ELSE 'Other'\n  END as \"Operation\",\n  LEFT(message, 150) as \"Details\"\nFROM cirislens.service_logs\nWHERE service_name = 'cirisbilling'\n  AND timestamp BETWEEN $__timeFrom() AND $__timeTo()\n  AND message NOT LIKE '%/health%'\nORDER BY timestamp DESC\nLIMIT 100",
