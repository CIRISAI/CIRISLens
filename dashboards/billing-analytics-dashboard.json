          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT\n  time_bucket('5 minutes', timestamp) AS time,\n  CASE\n    WHEN message LIKE '%/v1/billing/credits/check%' THEN 'Credit Checks'\n    WHEN message LIKE '%/v1/billing/litellm/usage%' THEN 'LLM Usage'\n    WHEN message LIKE '%/v1/billing/charges%' THEN 'Charges'\n    WHEN message LIKE '%/v1/integrity%' THEN 'Integrity'\n    ELSE 'Other'\n  END AS metric,\n  COUNT(*) as value\nFROM cirislens.service_logs\nWHERE service_name = 'cirisbilling'\n  AND timestamp BETWEEN $__timeFrom() AND $__timeTo()\n  AND message NOT LIKE '%/health%'\nGROUP BY 1, 2\nORDER BY 1",
          "refId": "A"
        }
      ],
      "title": "Billing API Activity",
      "type": "timeseries"
    },
