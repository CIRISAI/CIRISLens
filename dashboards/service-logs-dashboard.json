{
  "annotations": {
    "list": []
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "asDropdown": false,
      "icon": "external link",
      "includeVars": false,
      "keepTime": true,
      "tags": [],
      "targetBlank": false,
      "title": "Back to Main",
      "type": "link",
      "url": "/d/ciris-main/ciris-telemetry-dashboard"
    }
  ],
  "panels": [
    {
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "id": 1,
      "title": "Total Logs (24h)",
      "type": "stat",
      "datasource": "PostgreSQL",
      "targets": [
        {
          "rawSql": "SELECT COUNT(*) as value FROM cirislens.service_logs WHERE timestamp > NOW() - INTERVAL '24 hours'",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "id": 2,
      "title": "Errors (24h)",
      "type": "stat",
      "datasource": "PostgreSQL",
      "targets": [
        {
          "rawSql": "SELECT COUNT(*) as value FROM cirislens.service_logs WHERE timestamp > NOW() - INTERVAL '24 hours' AND level IN ('ERROR', 'CRITICAL')",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "id": 3,
      "title": "Warnings (24h)",
      "type": "stat",
      "datasource": "PostgreSQL",
      "targets": [
        {
          "rawSql": "SELECT COUNT(*) as value FROM cirislens.service_logs WHERE timestamp > NOW() - INTERVAL '24 hours' AND level = 'WARNING'",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50 }
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "id": 4,
      "title": "Active Services",
      "type": "stat",
      "datasource": "PostgreSQL",
      "targets": [
        {
          "rawSql": "SELECT COUNT(DISTINCT service_name) as value FROM cirislens.service_logs WHERE timestamp > NOW() - INTERVAL '1 hour'",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "id": 5,
      "title": "Log Volume by Service",
      "type": "timeseries",
      "datasource": "PostgreSQL",
      "targets": [
        {
          "rawSql": "SELECT time_bucket('5 minutes', timestamp) AS time, service_name, COUNT(*) as count FROM cirislens.service_logs WHERE $__timeFilter(timestamp) GROUP BY 1, 2 ORDER BY 1",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "never",
            "stacking": { "mode": "none" }
          },
          "unit": "short"
        }
      }
    },
    {
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "id": 6,
      "title": "Log Level Distribution",
      "type": "timeseries",
      "datasource": "PostgreSQL",
      "targets": [
        {
          "rawSql": "SELECT time_bucket('5 minutes', timestamp) AS time, level, COUNT(*) as count FROM cirislens.service_logs WHERE $__timeFilter(timestamp) GROUP BY 1, 2 ORDER BY 1",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 100,
            "stacking": { "mode": "normal" }
          },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "ERROR" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "CRITICAL" },
            "properties": [{ "id": "color", "value": { "fixedColor": "dark-red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "WARNING" },
            "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "INFO" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "DEBUG" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          }
        ]
      }
    },
    {
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 12 },
      "id": 7,
      "title": "Logs by Service (24h)",
      "type": "piechart",
      "datasource": "PostgreSQL",
      "targets": [
        {
          "rawSql": "SELECT service_name, COUNT(*) as count FROM cirislens.service_logs WHERE timestamp > NOW() - INTERVAL '24 hours' GROUP BY service_name ORDER BY count DESC",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right" },
        "pieType": "pie"
      }
    },
    {
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 12 },
      "id": 8,
      "title": "Top Events (24h)",
      "type": "bargauge",
      "datasource": "PostgreSQL",
      "targets": [
        {
          "rawSql": "SELECT COALESCE(event, 'unspecified') as event, COUNT(*) as count FROM cirislens.service_logs WHERE timestamp > NOW() - INTERVAL '24 hours' AND event IS NOT NULL GROUP BY event ORDER BY count DESC LIMIT 10",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal"
      }
    },
    {
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 12 },
      "id": 9,
      "title": "Service Health",
      "type": "table",
      "datasource": "PostgreSQL",
      "targets": [
        {
          "rawSql": "SELECT service_name as \"Service\", COUNT(*) as \"Total\", COUNT(*) FILTER (WHERE level IN ('ERROR', 'CRITICAL')) as \"Errors\", MAX(timestamp) as \"Last Seen\" FROM cirislens.service_logs WHERE timestamp > NOW() - INTERVAL '1 hour' GROUP BY service_name ORDER BY \"Errors\" DESC",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Errors" },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": { "type": "color-background" }
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    { "color": "green", "value": null },
                    { "color": "yellow", "value": 1 },
                    { "color": "red", "value": 10 }
                  ]
                }
              }
            ]
          }
        ]
      }
    },
    {
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 18 },
      "id": 10,
      "title": "Recent Logs",
      "type": "table",
      "datasource": "PostgreSQL",
      "targets": [
        {
          "rawSql": "SELECT timestamp as \"Time\", service_name as \"Service\", level as \"Level\", COALESCE(event, '-') as \"Event\", LEFT(message, 200) as \"Message\" FROM cirislens.service_logs WHERE $__timeFilter(timestamp) ORDER BY timestamp DESC LIMIT 100",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Level" },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": { "type": "color-text" }
              },
              {
                "id": "mappings",
                "value": [
                  { "type": "value", "options": { "ERROR": { "color": "red", "text": "ERROR" } } },
                  { "type": "value", "options": { "CRITICAL": { "color": "dark-red", "text": "CRITICAL" } } },
                  { "type": "value", "options": { "WARNING": { "color": "yellow", "text": "WARNING" } } },
                  { "type": "value", "options": { "INFO": { "color": "green", "text": "INFO" } } },
                  { "type": "value", "options": { "DEBUG": { "color": "blue", "text": "DEBUG" } } }
                ]
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "cellHeight": "sm"
      }
    },
    {
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 28 },
      "id": 11,
      "title": "Recent Errors",
      "type": "table",
      "datasource": "PostgreSQL",
      "targets": [
        {
          "rawSql": "SELECT timestamp as \"Time\", service_name as \"Service\", COALESCE(event, '-') as \"Event\", message as \"Message\", attributes as \"Details\" FROM cirislens.service_logs WHERE $__timeFilter(timestamp) AND level IN ('ERROR', 'CRITICAL') ORDER BY timestamp DESC LIMIT 50",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "red" }
        }
      },
      "options": {
        "showHeader": true,
        "cellHeight": "sm"
      }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 30,
  "style": "dark",
  "tags": ["service-logs", "ciris"],
  "templating": {
    "list": [
      {
        "current": { "selected": true, "text": "All", "value": "$__all" },
        "datasource": "PostgreSQL",
        "definition": "SELECT DISTINCT service_name FROM cirislens.service_logs ORDER BY service_name",
        "hide": 0,
        "includeAll": true,
        "label": "Service",
        "multi": true,
        "name": "service",
        "query": "SELECT DISTINCT service_name FROM cirislens.service_logs ORDER BY service_name",
        "refresh": 1,
        "type": "query"
      },
      {
        "current": { "selected": true, "text": "All", "value": "$__all" },
        "hide": 0,
        "includeAll": true,
        "label": "Level",
        "multi": true,
        "name": "level",
        "options": [
          { "selected": false, "text": "DEBUG", "value": "DEBUG" },
          { "selected": false, "text": "INFO", "value": "INFO" },
          { "selected": false, "text": "WARNING", "value": "WARNING" },
          { "selected": false, "text": "ERROR", "value": "ERROR" },
          { "selected": false, "text": "CRITICAL", "value": "CRITICAL" }
        ],
        "type": "custom"
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Service Logs",
  "uid": "service-logs",
  "version": 1
}
